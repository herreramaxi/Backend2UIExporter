<!-- 
  This file is automatically imported by NuGet into a user's project
  when it targets a single framework, or in classic (pre 2017) csproj projects.
-->
<Project>	
	<Target Name="MyTasksAfterbuild2" AfterTargets="AfterBuild">
		<Message Text="(CustomTask) MSBuildThisFileDirectory: '$(MSBuildThisFileDirectory)'" Importance="High" />
		<Message Text="(CustomTask) ReferencePath: '$(ReferencePath)'" Importance="High" />
		<Message Text="(CustomTask) IntermediateAssembly: '@(IntermediateAssembly)'" Importance="High" />
		<Message Text="(CustomTask) MSBuildProjectDirectory: '@(MSBuildProjectDirectory)'" Importance="High" />
		<Message Text="(CustomTask) MSBuildRuntimeType: '$(MSBuildRuntimeType)'" Importance="High" />	
	</Target>
	
</Project>